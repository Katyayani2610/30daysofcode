class Solution {
  public:
    void printTriangle(int n) {
        for(int i=0; i<n; i++)
        {
            char ch=64+n;
            for(int j=0;j<=i;j++)
            
            {
                cout<<ch<<" ";
                ch--;
            }
            cout<<endl;
        }
    }
};
