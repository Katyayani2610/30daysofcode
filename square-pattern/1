https://practice.geeksforgeeks.org/problems/square-pattern/1

#include <iostream>
using namespace std;

void printSquare(int n) {
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            cout << "* ";
        }
        cout << endl;  // Move to the next line after each row
    }
}

int main() {
    int n = 5;  // You can change the value of n as needed
    printSquare(n);
    return 0;
}
