class Solution{
public:
	
	void printTriangle(int n) {
	    for( int i=0; i<n;i++)
	    {
	        for( int j=n;j>i;j--)
	        {
	            cout<<n-j+1<<" ";
	           
	        }
	        cout<<endl;
	    }
	}
};
